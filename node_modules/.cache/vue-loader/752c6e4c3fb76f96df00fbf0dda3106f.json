sult: { loading, error, data } }\">\n      <!-- Loading -->\n      <div v-if=\"loading\" class=\"loading apollo\">Loading...</div>\n\n      <!-- Error -->\n      <div v-else-if=\"error\" class=\"error apollo\">An error occured</div>\n\n      <!-- Result -->\n      <div v-else-if=\"data\" class=\"result apollo\">\n        <v-row>\n          <v-col cols=\"4\" v-for=\"(item, i) in data.courses\" :key=\"i\">\n            <v-card class=\"mx-auto\" max-width=\"350\">\n              <v-card-text>\n                <div>{{ item.courseCode }}</div>\n                <p class=\"courseName\">\n                  {{ item.name }}\n                </p>\n                <p>{{ item.termsOffered }}</p>\n                <div class=\"text--primary\">\n                  {{ item.description | truncate(200) }}\n                </div>\n              </v-card-text>\n              <v-card-actions>\n                <v-btn color=\"primary\" fab x-small dark @click=\"editCourse(item)\">\n              <v-icon>mdi-pencil</v-icon>\n            </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-col>\n        </v-row>\n      </div>\n\n      <!-- No result -->\n      <div v-else class=\"no-result apollo\">No result :(</div>\n    </template>\n  </ApolloQuery>\n</template>\n\n<script>\n//import vue-truncate-filter from 'vue-truncate-filter'\n\nexport default {\n  name: 'Home',\n\n  data: () => ({\n    searchString: \"DGM\"\n  }),\n  methods: {\n    editCourse(course) {\n      this.$store.dispatch('editCourse', course)\n      this.$router.push('admin')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.result {\n  padding: 1rem;\n}\n\n.courseName {\n  font-size: 1.25rem;\n  font-weight: 600;\n}\n</style>\n"]}]}